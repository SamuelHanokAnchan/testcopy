{
  "name": "showroom-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:all",
    "start:client": "ng serve --proxy-config proxy.conf.json",
    "start:image": "node scripts/start-image-service.cjs",
    "start:all": "concurrently -k -n IMAGE,WEB -c cyan,magenta \"npm:start:image\" \"npm:start:client\"",
    "build": "ng build --configuration production",
    "build:prod": "node scripts/gen-env.mjs && npm run build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:showroom-demo": "node dist/showroom-demo/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "engines": {
    "node": ">=20.19.0 <21"
  },
  "dependencies": {
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/platform-server": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@angular/ssr": "^20.1.6",
    "@syncfusion/ej2-angular-base": "^30.1.37",
    "@syncfusion/ej2-angular-buttons": "^30.1.37",
    "@syncfusion/ej2-angular-calendars": "^30.1.37",
    "@syncfusion/ej2-angular-charts": "^30.1.39",
    "@syncfusion/ej2-angular-dropdowns": "^30.1.39",
    "@syncfusion/ej2-angular-grids": "^30.1.39",
    "@syncfusion/ej2-angular-image-editor": "^30.1.37",
    "@syncfusion/ej2-angular-inputs": "^30.1.40",
    "@syncfusion/ej2-angular-layouts": "^30.1.37",
    "@syncfusion/ej2-angular-lists": "^30.2.4",
    "@syncfusion/ej2-angular-maps": "^30.1.37",
    "@syncfusion/ej2-angular-navigations": "^30.1.39",
    "@syncfusion/ej2-angular-notifications": "^31.1.17",
    "@syncfusion/ej2-angular-pdfviewer": "^30.2.7",
    "@syncfusion/ej2-angular-schedule": "^30.1.42",
    "@syncfusion/ej2-angular-splitbuttons": "^30.1.39",
    "@syncfusion/ej2-angular-treegrid": "^30.1.38",
    "@syncfusion/ej2-base": "^30.1.38",
    "@syncfusion/ej2-icons": "^30.2.4",
    "@syncfusion/ej2-layouts": "^30.1.37",
    "@syncfusion/ej2-maps": "^30.1.37",
    "@tailwindcss/cli": "^4.1.12",
    "@tailwindcss/postcss": "^4.1.12",
    "express": "^5.1.0",
    "pdf-lib": "^1.17.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.6",
    "@angular/cli": "^20.1.6",
    "@angular/compiler-cli": "^20.1.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.0",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.2"
  }
}
